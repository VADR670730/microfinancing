        <record id="members_form_view" model="ir.ui.view">
            <field name="name">res.partner.inherit.form</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//button[@name='toggle_active']" position="before">
                    <button class="oe_stat_button"
                            icon="fa-user"
                            groups="credit_base.group_credit_do"
                            context="{'partner_id': active_id, 'partner_name': name}">
                        <field nolabel="1" name="type_str" widget="statinfo"/>
                    </button>
                </xpath>
                <xpath expr="//field[@name='function']" position="attributes">
                    <attribute name="groups">credit_base.group_credit_sysad</attribute>
                </xpath>
                <xpath expr="//field[@name='vat']" position="attributes">
                    <attribute name="groups">credit_base.group_credit_sysad</attribute>
