        <record id="care_groups_tree_view" model="ir.ui.view">
            <field name="name">credit.loan.group.list</field>
            <field name="model">credit.loan.group</field>
            <field name="arch" type="xml">
                <tree string="List of CARE Groups">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="care_groups_form_view" model="ir.ui.view">
            <field name="name">credit.loan.group.form</field>
            <field name="model">credit.loan.group</field>
            <field name="arch" type="xml">
                <form string="CARE Group">
                    <header>
                        <field name="state" widget="statusbar" statusbar_visible="draft,approve"/>
                    </header>
                    <sheet>
                        <h1>
                            <field name="code"/>
                        </h1>
                        <group>
                            <group>
                                <field name="event_registration_id"/>
                                <field name="creator"/>
                                <field name="date_organized"/>
                                <field name="date_approved"/>
                                <field name="contact_person"/>
                                <field name="group_leader"/>
                                <field name="do"/>
                                <field name="area_id"/>
                            </group>
                            <group>
                                <field name="street"/>
                                <field name="street2"/>
                                <field name="zip"/>
                                <field name="city"/>
                                <field name="state_id"/>
                                <field name="country_id"/>
                            </group>
                        </group>
                        <notebook>
                            <page string="Members">
                                <group>
                                    <field name="members"/>
                                </group>
                            </page>
                            <page string="Loan Account">
                                <group>
                                    <field name="loan_account"/>
                                </group>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="credit_base.action_window_care_groups" model="ir.actions.act_window">
