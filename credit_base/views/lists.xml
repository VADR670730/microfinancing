        <record id="branch_tree_view" model="ir.ui.view">
            <field name="name">res.branch.list</field>
            <field name="model">res.branch</field>
            <field name="arch" type="xml">
                <tree string="List of Branch">
                    <field name="name"/>
                    <field name="city"/>
                    <field name="manager"/>
                </tree>
            </field>
        </record>

        <record id="branch_form_view" model="ir.ui.view">
            <field name="name">res.branch.form</field>
            <field name="model">res.branch</field>
            <field name="arch" type="xml">
                <form string="Branch">
                    <sheet>
                        <h1>
                            <field name="name"/>
                        </h1>
                        <group>
                            <field name="company_id"/>
                            <field name="related_partner"/>
                        </group>
                        <group>
                            <field name="code"/>
                            <field name="email"/>
                            <field name="phone"/>
                            <field name="website"/>
                            <field name="vat"/>
                        </group>
                        <group>
                            <field name="street"/>
                            <field name="street2"/>
                            <field name="city"/>
                            <field name="state_id"/>
                            <field name="country_id"/>
                        </group>
                        <notebook>
                            <page string="Manager">
                                <field name="manager_id"/>
                            </page>
                            <page string="Financing Accounts">
                                <field name="financing_ids"/>
                            </page>
                            <page string="Area">
                                <field name="area_ids"/>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>

