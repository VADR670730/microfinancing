        <record id="care_groups_tree_view" model="ir.ui.view">
            <field name="name">credit.loan.group.list</field>
            <field name="model">credit.loan.group</field>
            <field name="arch" type="xml">
                <tree string="List of CARE Groups">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="care_groups_form_view" model="ir.ui.view">
            <field name="name">credit.loan.group.form</field>
            <field name="model">credit.loan.group</field>
            <field name="arch" type="xml">
                <form string="CARE Group">
                    <header>
                        <button name="confirm_group" string="Confirm" type="object" class="oe_highlight"  states="draft"/>
                        <button name="draft_group" string="Send to Draft" type="object" states="confirm"/>
                        <field name="status" widget="statusbar" statusbar_visible="draft,confirm"/>
                    </header>
                    <sheet>
                        <div class="oe_button_box" name="button_box" groups="base.group_user">
                            <button name="toggle_active" type="object" class="oe_stat_button" icon="fa-archive">
                                <field name="state" widget="boolean_button" options="{&quot;terminology&quot;: &quot;archive&quot;}"/>
                            </button>
                        </div>
                        <div class="oe_title">
                            <label for="code" class="oe_edit_only"/>
                            <h1><field name="code" placeholder="Group Code"/></h1>
                        </div>
                        <group>
                            <group>
                                <field name="event_registration_id"/>
                                <field name="creator"/>
                                <field name="date_organized"/>
                                <field name="date_approved"/>
                                <field name="contact_person"/>
                                <field name="group_leader"/>
                                <field name="do"/>
                                <field name="area_id"/>
                            </group>
                            <group>
                                <field name="street"/>
                                <field name="street2"/>
                                <field name="zip"/>
                                <field name="city"/>
                                <field name="state_id"/>
                                <field name="country_id"/>
                            </group>
                        </group>
                        <notebook>
                            <page string="Members">
                                <group>
                                    <field name="members"/>
                                </group>
                            </page>
                            <page string="Loan Account">
                                <group>
                                    <field name="financing_ids"/>
                                </group>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>

