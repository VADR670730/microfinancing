            string="Recruitment and Resource Plan"
            report_type="qweb-pdf"
            name="hrms_employee.report_resource_template"
            file="hrms_employee.report_resource_template"
			paperformat="paperformat_legal_employee"/>

		<report
            id="report_recruitment_agreement_id"
            model="hr.recruitment.agreement"
            string="Recruitment Agreement"
            report_type="qweb-pdf"
            name="hrms_employee.report_recruitment_agreement_template"
            file="hrms_employee.report_recruitment_agreement_template"
			paperformat="paperformat_hr_paper_letter_margin"
			groups="hr.group_hr_user,hr.group_hr_manager"/>
		<report
            id="report_personnel_requisition_form_id"
            model="hr.recruitment.request"
