            report_type="qweb-pdf"
            name="hrms_employee.personnel_requisition_form_template"
            file="hrms_employee.personnel_requisition_form_template"
			paperformat="paperformat_hr_paper_letter_margin"
			groups="hr.group_hr_user,hr.group_hr_manager"/>
    </data>
</odoo>