<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <!--HRMS Legal Paper LandscapeFormat-->
        <record id="paperformat_legal_employee" model="report.paperformat">
		    <field name="name">LEGAL HR EMPLOYEE</field>
		    <field name="format">custom</field>
            <field name="page_height">330</field>
            <field name="page_width">216</field>
		    <field name="orientation">Landscape</field>
		    <field name="margin_top">24</field>
		    <field name="margin_bottom">24</field>
		    <field name="margin_left">14</field>
		    <field name="margin_right">14</field>
		    <field name="header_line" eval="False"/>
		    <field name="header_spacing">24</field>
		    <field name="dpi">100</field>
		</record>

		<record id="paperformat_hr_legal_landscape" model="report.paperformat">
		    <field name="name">LEGAL HR LANDSCAPE</field>
		    <field name="format">custom</field>
            <field name="page_height">330</field>
            <field name="page_width">216</field>
		    <field name="orientation">Landscape</field>
		    <field name="margin_top">12</field>
		    <field name="margin_bottom">12</field>
		    <field name="margin_left">12</field>
		    <field name="margin_right">12</field>
		    <field name="header_line" eval="False"/>
		    <field name="header_spacing">12</field>
		    <field name="dpi">100</field>
		</record>

        <!--HRMS Legal Portrait Paper Format-->
        <record id="paperformat_legal_employee_portrait" model="report.paperformat">
		    <field name="name">LEGAL Portrait HR EMPLOYEE</field>
		    <field name="format">custom</field>
            <field name="page_height">330</field>
            <field name="page_width">216</field>
            <field name="orientation">Portrait</field>
		    <field name="margin_top">24</field>
		    <field name="margin_bottom">24</field>
		    <field name="margin_left">14</field>
		    <field name="margin_right">14</field>
		    <field name="header_line" eval="False"/>
		    <field name="header_spacing">24</field>
		    <field name="dpi">100</field>
		</record>

		<!--HRMS Legal Portrait Paper Format w/ Margin-->
        <record id="paperformat_legal_employee_margin" model="report.paperformat">
		    <field name="name">Portrait Legal with Margin</field>
		    <field name="format">custom</field>
            <field name="page_height">330</field>
            <field name="page_width">216</field>
            <field name="orientation">Portrait</field>
		    <field name="margin_top">14</field>
		    <field name="margin_bottom">14</field>
		    <field name="margin_left">24</field>
		    <field name="margin_right">24</field>
		    <field name="header_line" eval="False"/>
		    <field name="header_spacing">4</field>
		    <field name="dpi">100</field>
		</record>

		<record id="paperformat_portrait_legal_margins" model="report.paperformat">
		    <field name="name">Portrait Legal with Margin 7</field>
		    <field name="format">custom</field>
            <field name="page_height">330</field>
            <field name="page_width">216</field>
            <field name="orientation">Portrait</field>
		    <field name="margin_top">24</field>
		    <field name="margin_bottom">14</field>
		    <field name="margin_left">14</field>
		    <field name="margin_right">14</field>
		    <field name="header_line" eval="False"/>
		    <field name="header_spacing">24</field>
		    <field name="dpi">100</field>
		</record>

		<record id="paperformat_portrait_legal_paf" model="report.paperformat">
		    <field name="name">Portrait Legal PAF</field>
		    <field name="format">custom</field>
            <field name="page_height">330</field>
            <field name="page_width">216</field>
            <field name="orientation">Portrait</field>
		    <field name="margin_top">14</field>
		    <field name="margin_bottom">14</field>
		    <field name="margin_left">14</field>
		    <field name="margin_right">14</field>
		    <field name="header_line" eval="False"/>
		    <field name="header_spacing">14</field>
		    <field name="dpi">100</field>
		</record>

		<!--Payroll Letter Paper Format-->
		<record id="paperformat_hr_paper_letter" model="report.paperformat">
		    <field name="name">HR Letter Paper Portrait</field>
            <field name="default" eval="True" />
            <field name="format">custom</field>
            <field name="page_height">270</field>
            <field name="page_width">216</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">24</field>
            <field name="margin_bottom">26</field>
            <field name="margin_left">24</field>
            <field name="margin_right">24</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">4</field>
            <field name="dpi">100</field>
		</record>

		<record id="paperformat_hr_paper_letter_margin" model="report.paperformat">
		    <field name="name">HR Letter Paper Portrait Margin</field>
            <field name="default" eval="True"/>
            <field name="format">custom</field>
            <field name="page_height">270</field>
            <field name="page_width">216</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">4</field>
            <field name="margin_bottom">4</field>
            <field name="margin_left">24</field>
            <field name="margin_right">24</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">4</field>
            <field name="dpi">100</field>
		</record>

		<report id="dev_emp_report_print_id"
            model="hr.employee"
            string="Employee Details"
            report_type="qweb-pdf"
            name="hrms_employee.dev_emp_profile_report_template"
            file="hrms_employee.dev_emp_profile_report_template"
			paperformat="paperformat_hr_paper_letter"
			groups="hr.group_hr_user,hr.group_hr_manager"/>

        <!--HRMS MONTHLY REPORT-->
        <report
            id="report_hr_employee_monthly"
            model="hr.employee.list.handler"
            string="Employee List Report"
            report_type="qweb-pdf"
            name="hrms_employee.report_hr_employee_template"
            file="hrms_employee.report_hr_employee_template"
            paperformat="paperformat_legal_employee"
			groups="hr.group_hr_user,hr.group_hr_manager"/>

		<report
            id="report_hr_employee_headcount"
            model="hr.employee.headcount.handler"
            string="Employee Headcount"
            report_type="qweb-pdf"
            name="hrms_employee.report_hr_employee_headcount_template"
            file="hrms_employee.report_hr_employee_headcount_template"
            paperformat="paperformat_legal_employee"
			groups="hr.group_hr_user,hr.group_hr_manager"/>

		<report
            id="report_employee_contract"
            string="Employee Contract"
            model="hr.contract"
            report_type="qweb-pdf"
            file="hrms_employee.report_contract"
            name="hrms_employee.report_contract"
			groups="hr.group_hr_user,hr.group_hr_manager"/>

		<report
            id="hr_monthly_contract_handlers"
