			groups="hr.group_hr_user,hr.group_hr_manager"/>

		<report
            id="report_new_contract_id"
            model="hr.contract"
            string="HR Form 106"
            report_type="qweb-pdf"
            name="hrms_employee.report_contract_template"
            file="hrms_employee.report_contract_template"
            paperformat="paperformat_legal_employee_portrait"
			groups="hrms_employee.group_hr_contract_personnel"/>

		<report
            id="report_new_contracts_id"
            model="hr.contract.partial"
            string="HR Form 106"
            report_type="qweb-pdf"
            name="hrms_employee.report_my_paf_template"
            file="hrms_employee.report_my_paf_template"
            paperformat="paperformat_portrait_legal_paf"
			groups="hrms_employee.group_hr_contract_personnel"/>

		<report
            id="report_non_disclosure_id"
            model="hr.non.disclosure.agreement"
            string="HR Form 123"
            report_type="qweb-pdf"
            name="hrms_employee.report_non_disclosure_template"
            file="hrms_employee.report_non_disclosure_template"
            paperformat="paperformat_legal_employee_margin"
			groups="hrms_employee.group_hr_contract_personnel"/>

		<report
            id="report_prob_employment_contract_id"
            model="hr.probationary.employment.contract"
            string="Probationary Employment Contract"
            report_type="qweb-pdf"
            name="hrms_employee.report_probationary_employment_template"
            file="hrms_employee.report_probationary_employment_template"
            paperformat="paperformat_portrait_legal_paf"
			groups="hrms_employee.group_hr_contract_personnel"/>

		<report
            id="report_memorandum_agreement_id"
            model="hr.memorandum.agreement"
            string="Memorandum of Agreement"
            report_type="qweb-pdf"
            name="hrms_employee.report_memorandum_agreement_template"
            file="hrms_employee.report_memorandum_agreement_template"
            paperformat="paperformat_legal_employee_margin"
			groups="hrms_employee.group_hr_contract_personnel"/>

		<report
            id="report_insurance_waiver_id"
            model="hr.employee"
            string="Insurance Waiver"
            report_type="qweb-pdf"
            name="hrms_employee.insurance_waiver_template"
            file="hrms_employee.insurance_waiver_template"
            paperformat="paperformat_legal_employee_margin"
			groups="hrms_employee.group_hr_contract_personnel"/>

		<report
            id="report_check_id"
            model="hr.gift.check.handler"
            string="Employee's Benefits"
            report_type="qweb-pdf"
            name="hrms_employee.report_check_template"
            file="hrms_employee.report_check_template"
            paperformat="paperformat_portrait_legal_margins"
			groups="hr.group_hr_user,hr.group_hr_manager"/>

		<!--Recruitment Form Report-->

		<report
            id="report_hr_manpower_plan_id"
            model="hr.manpower.plan.handler"
            string="Manpower Plan"
            report_type="qweb-pdf"
            name="hrms_employee.report_man_power_template"
            file="hrms_employee.report_man_power_template"
            paperformat="paperformat_hr_legal_landscape"
			groups="hr_recruitment.group_hr_recruitment_manager,hr_recruitment.group_hr_recruitment_user"/>

        <report
            id="hr_applicant_handlers"
            model="hr.recruitment.handler"
            string="Applicant Monitoring Report"
            report_type="qweb-pdf"
            name="hrms_employee.report_recruitment_template"
            file="hrms_employee.report_recruitment_template"
            paperformat="paperformat_legal_employee"/>

		<report
            id="hr_resource_handlers"
